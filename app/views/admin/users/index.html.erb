<h2>会員一覧</h2>
 <% @users.each do |users| %>
  <table>
    <thead>
      <tr>
        <th>プロフィール画像</th>
        <th>名前</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <%= link_to admin_user_path(users) do %>
            <% if users.profile_image.attached? %>
             <%= image_tag users.profile_image, size: "150x150" %>
            <% else%>
             <%= image_tag 'no-image', size: "150x150" %>
            <% end %>
        </td>
        <td><%= users.name %></td>
        <td><%= users.email %></td>
        <td>
          <% if users.is_deleted == false %>
           有効
          <% else %>
           退会
          <% end %>
        </td>
          <% end %>
      </tr>
    </tbody>
  </table>
 <% end %>